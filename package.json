{
  "name": "paretojs",
  "version": "0.8.2",
  "description": "A lightweight truly functional utility library that follows the 80/20 principle.",
  "main": "src/pareto.js",
  "scripts": {
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- --timeout 30000 test/**/*.spec.js",
    "build": "webpack -p",
    "start": "webpack",
    "prepublish": "npm run build",
    "test": "npm run coverage",
    "test-unit": "mocha --compilers js:babel-core/register test/**/*.spec.js",
    "test-unit-watch": "mocha --watch --compilers js:babel-core/register test/**/*.spec.js",
    "lint": "eslint src test"
  },
  "keywords": [
    "util",
    "utility",
    "functional",
    "light-weight",
    "stdlib",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/concretesolutions/pareto.js"
  },
  "authors": [
    "Matheus Lima <matheusml90@gmail.com> (https://matheuslima.com)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "~6.6.5",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "~6.2.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "~6.6.0",
    "babel-register": "~6.6.5",
    "cross-env": "~1.0.7",
    "eslint": "~2.3.0",
    "expect": "~1.14.0",
    "mocha": "~2.4.5",
    "webpack": "^1.12.14"
  }
}
