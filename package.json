{
  "name": "paretojs",
  "version": "1.0.4",
  "description": "A trully small functional utility library that only has what you really need.",
  "main": "dist/pareto.js",
  "scripts": {
    "test": "mocha test/**/*.spec.js --compilers js:babel-register",
    "test:watch": "npm t -- --watch",
    "coverage": "nyc npm t",
    "coverage:check": "nyc check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "coverage:report": "cat ./coverage/lcov.info | codecov",
    "lint": "eslint src test",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --out-dir dist src",
    "build:umd": "webpack --output-filename pareto.umd.js",
    "build:umd.min": "webpack --output-filename pareto.umd.min.js -p"
  },
  "keywords": [
    "util",
    "utility",
    "small",
    "functional",
    "immutable",
    "lightweight",
    "stdlib",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/concretesolutions/pareto.js"
  },
  "author": "Matheus Lima <matheusml90@gmail.com> (https://matheuslima.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.3.13",
    "codecov": "^1.0.1",
    "eslint": "^3.1.1",
    "expect": "~1.14.0",
    "mocha": "~2.4.5",
    "npm-run-all": "^2.3.0",
    "nyc": "^7.1.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.3.1"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
