{
  "name": "paretojs",
  "version": "0.8.13",
  "description": "A small trully functional utility library that only has what you really need.",
  "main": "src/pareto.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- --timeout 30000 test/**/*.spec.js",
    "build": "babel src --out-dir dist",
    "start": "webpack",
    "prepublish": "npm run build",
    "test": "npm run coverage",
    "test-unit": "mocha --compilers js:babel-core/register test/**/*.spec.js",
    "test-unit-watch": "mocha --watch --compilers js:babel-core/register test/**/*.spec.js",
    "lint": "eslint src test"
  },
  "keywords": [
    "util",
    "utility",
    "small",
    "functional",
    "immutable",
    "lightweight",
    "stdlib",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/concretesolutions/pareto.js"
  },
  "authors": [
    "Matheus Lima <matheusml90@gmail.com> (https://matheuslima.com)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-register": "^6.3.13",
    "cross-env": "~1.0.7",
    "es3ify": "^0.2.1",
    "eslint": "~2.3.0",
    "expect": "~1.14.0",
    "glob": "^7.0.3",
    "mocha": "~2.4.5",
    "webpack": "^1.12.14"
  },
  "npmName": "paretojs",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
}
